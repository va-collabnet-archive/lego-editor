	<!-- discernible can have rel's -->
	<lego>
		<legoUUID>8cd4b1c0-0be7-11e2-891e-0813900c9a66</legoUUID>
		<stamp status="" author="" time="2012-05-30T09:00:00" module="" path="" />
		<pncs value="6th ICS MCL" name="PHYSICAL EXAM_CARDIO_Standard_Cardiac_Exam_Abnormal_PMI" id="24265" />
		<assertion>
			<assertionUUID>7dd4b1c0-9be2-11e2-892e-0800200c9a66</assertionUUID>
			<discernible>
				<conceptAndRel sctid="248661003" desc="Apex beat displaced">
					<rel>
						<typeConcept sctid="363698007" desc="Finding site" />
						<!-- Each rel must contain a type, and either a concept or a measurement -->
						<!-- Concepts may be referenced by UUID, instead of sctid -->
						<concept uuid="8dd4b7c0-0be2-11e2-892e-1100200c9a66" desc="Sixth intercostal space midclavicular line" />
					</rel>
				</conceptAndRel>
			</discernible>
			<qualifier>
				<concept sctid="5880005" desc="Physical examination procedure" />
			</qualifier>
			<value>
				<measurement>
					<numericPoint>15</numericPoint>
				</measurement>
			</value>
			<timing>
				<!-- units are optional -->
				<measurement>
					<!-- String values are limited to values from an enumeration -->
					<stringPoint>NOW</stringPoint>
				</measurement>
			</timing>
		</assertion>

	</lego>

	<!-- real example with two rels -->
	<lego>
		<legoUUID>8fd4b1c0-0be7-11e2-891e-0813900c9a66</legoUUID>
		<stamp status="" author="" time="2012-05-30T09:00:00" module="" path="" />
		<pncs value="Every 5 to 6 months" name="HISTORY_MISC_Standard_Flareups_Frequency" id="24619" />
		<assertion>
			<assertionUUID>8fd4b1c0-0ce7-11e2-891e-0813900c9a66</assertionUUID>
			<discernible>
				<conceptAndRel sctid="404684003" desc="Clinical finding" >
					<rel>
						<typeConcept sctid="47429007" desc="Associated with" />
						<concept sctid="410661009" desc="Exacerbated" />
					</rel>
					<rel>
						<typeConcept sctid="260864003" desc="Frequency" />
						<measurement>
							<units>
								<concept sctid="258706009" desc="month" />
							</units>
							<interval>
								<greaterThan inclusive="true">
									<numericPoint>5</numericPoint>
								</greaterThan>
								<lessThan inclusive="true">
									<numericPoint>6</numericPoint>
								</lessThan>
							</interval>
						</measurement>
					</rel>
				</conceptAndRel>
			</discernible>
			<qualifier>
				<concept sctid="108217004" desc="Interview, history AND/OR physical examination" />
			</qualifier>
			<value>
				<measurement>
					<numericPoint>1</numericPoint>
				</measurement>
			</value>
			<timing>
				<!-- And we do ranges - greaterThan, lessThan, or both -->
				<measurement>
					<interval>
						<greaterThan inclusive="true">
							<numericPoint>5</numericPoint>
						</greaterThan>
					</interval>
				</measurement>
			</timing>
		</assertion>
	</lego>
		<lego>
		<legoUUID>8cd4b6c0-0be7-11e2-891e-0813200c9a66</legoUUID>
		<stamp status="" author="" time="2012-05-30T09:00:00" module="" path="" />

		<!-- pncs is required, and all 3 attributes are required -->
		<pncs value="Yes" name="stuff" id="5" />
		<assertion>
			<assertionUUID>8dd4b1c0-0be4-41e2-892e-0800200c9a66</assertionUUID>
			<discernible>
				<conceptAndRel sctid="67551009" desc="Abnormal third heart sound, S3">
					<rel>
						<typeConcept desc="foo" sctid="5"/>
						<!-- Numeric ranges can also have a relaxed range bound -->
						<measurement>
							<interval>
								<greaterThan inclusive="true">
									<pointPair>
										<greaterThan>5</greaterThan>
										<lessThan>6</lessThan>
									</pointPair>
								</greaterThan>
								<lessThan inclusive="false">
									<pointPair>
										<greaterThan>20</greaterThan>
										<lessThan>30.5</lessThan>
									</pointPair>
								</lessThan>
							</interval>
						</measurement>
					</rel>
				</conceptAndRel>
			</discernible>
			<qualifier>
				<concept sctid="5880005" desc="Physical examination procedure" />
			</qualifier>
			<value>
				<!-- Ranges can also have text strings -->
				<measurement>
					<interval>
						<greaterThan inclusive="true">
							<stringPoint>DOB</stringPoint>
						</greaterThan>
						<lessThan inclusive="false">
							<stringPoint>end active service</stringPoint>
						</lessThan>
					</interval>
				</measurement>
			</value>
			<timing>
				<!-- measurement range -->
				<measurement>
					<interval>
						<greaterThan inclusive="true">
							<numericPoint>5</numericPoint>
						</greaterThan>
						<lessThan inclusive="false">
							<numericPoint>25</numericPoint>
						</lessThan>
					</interval>
				</measurement>
			</timing>
		</assertion>
	</lego>
</legoList>