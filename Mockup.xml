	<!-- discernible can have rel's -->
	<lego>
		<legoUUID>8cd4b1c0-0be7-11e2-891e-0813900c9a66</legoUUID>
		<stamp status="" author="" time="2012-05-30T09:00:00" module="" path="" />
		<pncs value="6th ICS MCL" name="PHYSICAL EXAM_CARDIO_Standard_Cardiac_Exam_Abnormal_PMI" id="24265" />
		<assertion>
			<assertionUUID>7dd4b1c0-9be2-11e2-892e-0800200c9a66</assertionUUID>
			<discernible>
				<concept sctid="248661003" desc="Apex beat displaced">
					<rel>
						<type>
							<concept sctid="363698007" desc="Finding site" />
						</type>
						<!-- Each rel must contain a type, and either a concept or a measurement -->
						<!-- Concepts may be referenced by UUID, instead of sctid -->
						<destination>
							<concept uuid="8dd4b7c0-0be2-11e2-892e-1100200c9a66" desc="Sixth intercostal space midclavicular line" />
						</destination>
					</rel>
				</concept>
			</discernible>
			<qualifier>
				<concept sctid="5880005" desc="Physical examination procedure" />
			</qualifier>
			<value>
				<measurement>
					<point>
						<numericValue>15</numericValue>
					</point>
				</measurement>
			</value>
			<timing>
				<!-- units are optional -->
				<measurement>
					<!-- String values are limited to values from an enumeration -->
					<point>
						<stringValue>NOW</stringValue>
					</point>
				</measurement>
			</timing>
		</assertion>

	</lego>

	<!-- real example with two rels and a nested rel -->
	<lego>
		<legoUUID>8fd4b1c0-0be7-11e2-891e-0813900c9a66</legoUUID>
		<stamp status="" author="" time="2012-05-30T09:00:00" module="" path="" />
		<pncs value="Every 5 to 6 months" name="HISTORY_MISC_Standard_Flareups_Frequency" id="24619" />
		<assertion>
			<assertionUUID>8fd4b1c0-0ce7-11e2-891e-0813900c9a66</assertionUUID>
			<discernible>
				<concept sctid="404684003" desc="Clinical finding" >
					<rel>
						<type>
							<concept sctid="47429007" desc="Associated with" />
						</type>
						<destination>
							<concept sctid="410661009" desc="Exacerbated">
								<!-- non-sensical example, but shows that rels can be nested -->
								<rel>
									<type>
										<concept sctid="3" desc="Modifier"/>
									</type>
									<destination>
										<concept sctid="5" desc="Extreme"/>
									</destination>
								</rel>
							</concept>
						</destination>
					</rel>
					<rel>
						<type>
						    <concept sctid="260864003" desc="Frequency" />
						</type>
						<destination>
							<measurement>
								<units>
									<concept sctid="258706009" desc="month" />
								</units>
								<interval>
									<lowerPoint>
										<numericValue>5</numericValue>
									</lowerPoint>
									<upperPoint>
										<numericValue>6</numericValue>	
									</upperPoint>
								</interval>
							</measurement>
						</destination>
					</rel>
				</concept>
			</discernible>
			<qualifier>
				<concept sctid="108217004" desc="Interview, history AND/OR physical examination" />
			</qualifier>
			<value>
				<measurement>
					<point>
						<numericValue>1</numericValue>
					</point>
				</measurement>
			</value>
			<timing>
				<!-- And we do ranges - greaterThan, lessThan, or both -->
				<measurement>
					<interval>
						<lowerPoint>
							<numericValue>5</numericValue>
						</lowerPoint>
					</interval>
				</measurement>
			</timing>
		</assertion>
	</lego>
		<lego>
		<legoUUID>8cd4b6c0-0be7-11e2-891e-0813200c9a66</legoUUID>
		<stamp status="" author="" time="2012-05-30T09:00:00" module="" path="" />

		<!-- pncs is required, and all 3 attributes are required -->
		<pncs value="Yes" name="stuff" id="5" />
		<assertion>
			<assertionUUID>8dd4b1c0-0be4-41e2-892e-0800200c9a66</assertionUUID>
			<discernible>
				<concept sctid="67551009" desc="Abnormal third heart sound, S3">
					<rel>
						<type>
						    <concept desc="foo" sctid="5"/>
						</type>
						<!-- Numeric ranges can also have two sets of ranges -->
						<destination>
							<measurement>
								<interval>
									<lowerBound>
										<lowerPoint inclusive="true">
											<numericValue>5</numericValue>
										</lowerPoint>
										<upperPoint>
											<numericValue>6</numericValue>
										</upperPoint>
									</lowerBound>
									<upperBound>
										<lowerPoint>
											<numericValue>20</numericValue>
										</lowerPoint>
										<upperPoint inclusive="true">
											<numericValue>30.5</numericValue>
										</upperPoint>
									</upperBound>
								</interval>
							</measurement>
						</destination>
					</rel>
				</concept>
			</discernible>
			<qualifier>
				<concept sctid="5880005" desc="Physical examination procedure" />
			</qualifier>
			<value>
				<!-- Ranges can also have text strings -->
				<measurement>
					<interval>
						<lowerPoint>
							<stringValue>DOB</stringValue>
						</lowerPoint>
						<upperPoint>
							<stringValue>end active service</stringValue>
						</upperPoint>
					</interval>
				</measurement>
			</value>
			<timing>
				<!-- measurement range -->
				<measurement>
					<interval>
						<lowerPoint inclusive="true">
							<numericValue>5</numericValue>
						</lowerPoint>
						<upperPoint inclusive="true">
							<numericValue>25</numericValue>
						</upperPoint>
					</interval>
				</measurement>
			</timing>
		</assertion>
	</lego>
</legoList>