<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?scenebuilder-stylesheet ../../../../../../resources/styles.css?>

<BorderPane fx:id="borderPane" prefHeight="450.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml" fx:controller="gov.va.legoEdit.gui.templates.TemplatesPaneController">
  <center>
    <StackPane prefHeight="150.0" prefWidth="200.0">
      <children>
        <ListView fx:id="filterResults">
          <StackPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </StackPane.margin>
        </ListView>
        <ProgressIndicator fx:id="filterProgress" maxHeight="100.0" maxWidth="100.0" visible="false" StackPane.alignment="CENTER">
          <StackPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </StackPane.margin>
        </ProgressIndicator>
      </children>
    </StackPane>
  </center>
  <top>
    <HBox>
      <children>
        <TextField fx:id="filterText" HBox.hgrow="ALWAYS">
          <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </HBox.margin>
        </TextField>
        <Button fx:id="filterButton" maxWidth="-Infinity" mnemonicParsing="false" prefWidth="75.0" text="Filter" HBox.hgrow="NEVER">
          <HBox.margin>
            <Insets bottom="5.0" right="5.0" top="5.0" />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
  </top>
</BorderPane>
